            </com.beardedhen.androidbootstrap.BootstrapButtonGroup>

            <com.beardedhen.androidbootstrap.BootstrapLabel
                android:id="@+id/bootstrapLabel9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                app:bootstrapBrand="info"
                app:bootstrapHeading="h6"
                app:bootstrapText="{fa_circle_o} 결제방법(필수 한가지 선택)"
                app:layout_constraintStart_toStartOf="@+id/bootstrapButtonGroup5"
                app:layout_constraintTop_toBottomOf="@+id/bootstrapButtonGroup5" />

            <com.beardedhen.androidbootstrap.BootstrapButtonGroup
